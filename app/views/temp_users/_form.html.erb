<%= form_for(@temp_user) do |f| %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_area :name %>
  </div>
  <div class="field">
    <%= f.label :pin %><br>
    <%= f.number_field :pin %>
  </div>
  <div class="field">
    <%= f.label :expiry_at %><br>
    <%= f.datetime_select :expiry_at %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
